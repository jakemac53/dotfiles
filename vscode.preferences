{
    "dart.sdkPath": "<some-path>",
    "dart.showTodos": false,
    "editor.formatOnSave": true,
    "editor.rulers": [
        80
    ],
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "dart.debugExternalLibraries": true,
    "html.format.wrapLineLength": 80,
    "markdownlint.config": {
        "MD013": false,
        "MD041": false,
        "MD002": false,
        "MD024": false,
        "MD034": false
    }
}
